    <modules>
        <module>rdfpro-core</module>
        <module>rdfpro-groovy</module>
        <module>rdfpro-jsonld</module>
        <module>rdfpro-tql</module>
        <module>rdfpro-dist</module>
        <module>rdfpro-rules</module>
    </modules>

</project>
